package Secretary;

import javax.swing.JPanel;
import javax.swing.Icon;
import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JLabel;

public class CreatingSemesterUI extends JPanel {
	private JPanel panel;
	private JButton button;

	/**
	 * Create the panel.
	 */
	public CreatingSemesterUI() {
		setLayout(null);
		
		panel = new JPanel();
		panel.setBounds(10, 47, 430, 242);
		add(panel);
		
		
		JLabel lblicon=new JLabel("Check Semester!");
		lblicon.setIcon(new ImageIcon("img\\homeui\\resetpassword.png"));
		button = new JButton((Icon) lblicon);
		button.setBounds(10, 13, 89, 23);
		add(button);

	}
}
